

type Ip {
  uuid: ID!
  created_at: Time!
  updated_at: Time!
  response_code: String!
  ip_address: String!
}


type Query {
  getIPDetails(ip: String!): [Ip]
}

type EnqueSuccess {
  ip: Ip
}

type EnqueError {
  ip: String!
  message: String!
}

union EnqueStatus = EnqueSuccess | EnqueError


type Mutation {
  enque(ips: [String!]): [EnqueStatus]
}


scalar Time
